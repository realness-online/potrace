{
  "name": "@realness.online/potrace",
  "version": "2.1.21",
  "description": "Potrace in Javascript",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/realness-online/potrace.git"
  },
  "keywords": [
    "potrace",
    "trace",
    "tracing",
    "svg",
    "bitmap",
    "posterization"
  ],
  "author": "scott.fryxell@hey.com",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/realness-online/potrace/issues"
  },
  "homepage": "https://github.com/realness-online/potrace#readme",
  "devDependencies": {
    "jest": "^27.1.0"
  },
  "dependencies": {
    "jimp": "^0.9.0"
  },
  "jest": {
    "notify": false,
    "notifyMode": "change",
    "bail": 0,
    "verbose": false,
    "collectCoverage": false,
    "restoreMocks": true,
    "moduleFileExtensions": [ "js" ],
    "coverageDirectory": "<rootDir>artifacts/unit/coverage",
    "cacheDirectory": "<rootDir>artifacts/unit/cache",
    "coverageThreshold": {
      "global": {
        "statements": 4.99,
        "branches": 0,
        "functions": 0,
        "lines": 4.9
      }
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/lib/$1"
    },
    "testMatch": [
      "**/tests/**/*.spec.(js)"
    ],
    "testURL": "http://localhost/"
  }

}
