{
  "name": "@realness.online/potrace",
  "version": "2.1.21",
  "description": "Potrace in Javascript",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/realness-online/potrace.git"
  },
  "keywords": [
    "potrace",
    "trace",
    "tracing",
    "svg",
    "bitmap",
    "posterization"
  ],
  "author": "scott.fryxell@hey.com",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/realness-online/potrace/issues"
  },
  "homepage": "https://github.com/realness-online/potrace#readme",
  "devDependencies": {
    "jest": "^27.1.0"
  },
  "dependencies": {
    "jimp": "^0.9.0"
  },
  "jest": {
    "notify": false,
    "notifyMode": "change",
    "bail": 0,
    "verbose": false,
    "collectCoverage": false,
    "restoreMocks": true,
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "setupFiles": [
      "<rootDir>/tests/vue/unit/setup.js"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "transformIgnorePatterns": [
      "\\.pnp\\.[^\\/]+$"
    ],
    "coverageDirectory": "<rootDir>/tests/vue/artifacts/unit/coverage",
    "cacheDirectory": "<rootDir>/tests/vue/artifacts/unit/cache",
    "collectCoverageFrom": [
      "resources/js/**/*.{js,vue}",
      "!resources/js/bootstrap.js",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 1,
        "branches": 1,
        "functions": 1,
        "lines": 1
      }
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/resources/js/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/vue/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  }

}
